type Entree{
	id: ID!
	numeroBand: String
	dateTimeEntree: String
	totalHt: Float
	totalTva: Float
	totalTtc: Float
	designation: String
	partenaire: Partenaire
	detailEntrees: [DetailEntree]
}

input EntreeInput {
    numeroBand: String
    dateTimeEntree: String
    designation: String
    totalHt: Float
    totalTva: Float
    totalTtc: Float
    partenaireId: ID!
    details: [DetailEntreeInput]
}